function findDeletedNumber(arr, mixArr){
     var result=''; 
     var sorted = mixArr.sort((a, b)=>a-b); 
     for (var i=0; i<arr.length; i++){
            if (arr[i]==sorted[i] && arr[i+1]!==sorted[i+1]){
               result=i+1;
            }
     } 
     //if (result=0) return 0; 
     return arr[result]==undefined? 0 : arr[result];  
}
